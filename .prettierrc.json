  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "arrowParens": "always",
  "printWidth": 130,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "singleQuote": false
      }
    }
  ]
}